{

  "manifest_version": 2,
  "name": "AddToOmniFocus2",
  "version": "0.1",

  "description": "Add To OmniFocus2 Firefox Extension (Unofficial)",
  "homepage_url": "https://jbking.org/",
  "icons": {
    "48": "icons/icon-48.png"
  },

  "permissions": [
    "tabs",
    "activeTab"
  ],

  "browser_action": {
    "default_icon":  "icons/icon-32.png",
    "default_title": "Add page to OmniFocus"
  },

  "background": {
    "scripts": ["background.js"]
  }

}
